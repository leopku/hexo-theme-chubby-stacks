  }
  var title = __(widget_cfg.name,false);%>

		<div class="widget-container widget_search styled boxed boxed-cream">
			<div class="inner">
				<%- _partial('../_widgets/' + widget_cfg.name,widget_cfg) -%>
			</div>
		</div>
		<%if (!cache && hexo.debug){%>
			<div class="widget-container widget_search styled boxed boxed-cream">
				<div class="inner">
					<!--widget_<%- widget_cfg.name %>-->
				</div>
			</div>

		<%}%>
<% }); %>
